#Performing Mutation
def mutate(child, mutation_rate):
mutated_child = []
for bit in child:
if random.random() < mutation_rate:
mutated_bit = 1 - bit # Flipping the bit
else:

mutated_bit = bit
mutated_child.append(mutated_bit)
return mutated_child
